{
  "name": "runnerty",
  "description": "Process orchestrator",
  "version": "2.1.0-rc2",
  "preferGlobal": true,
  "main": "index.js",
  "license": "MIT",
  "author": "Coderty",
  "homepage": "http://www.runnerty.io/",
  "directories": {
    "bin": "./bin"
  },
  "bin": {
    "runnerty": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:runnerty/runnerty.git"
  },
  "bugs": {
    "url": "https://github.com/runnerty/runnerty/issues"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "RUNNERTY_TEST=true jest",
    "test-watch": "RUNNERTY_TEST=true jest --watch"
  },
  "dependencies": {
    "ajv": "^5.5.2",
    "body-parser": "^1.18.2",
    "bytes": "^3.0.0",
    "commander": "^2.15.1",
    "express": "^4.16.3",
    "express-jwt": "^5.3.0",
    "fs-extra": "^5.0.0",
    "glob-to-regexp": "^0.4.0",
    "helmet": "^3.9.0",
    "ical2json": "^1.2.0",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "millisecond": "^0.1.2",
    "moment": "^2.22.1",
    "mongoose": "^5.0.16",
    "morgan": "^1.9.0",
    "object-sizeof": "^1.2.0",
    "redis": "^2.8.0",
    "request": "^2.85.0",
    "winston": "^2.4.2"
  },
  "keywords": [
    "runnerty",
    "scheduler",
    "task",
    "task manager",
    "automation",
    "job",
    "notifications",
    "executions",
    "sysadmin",
    "tools",
    "logs",
    "log",
    "reload",
    "programmatic",
    "production",
    "monitoring",
    "process manager",
    "profiling",
    "cron",
    "devops",
    "dev ops",
    "process orchestrator",
    "orchestrator"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "all-contributors-cli": "4.11.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.15.1",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/loadConfigSection.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
