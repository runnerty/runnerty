{
  "name": "runnerty",
  "description": "Process orchestrator",
  "version": "3.2.0-rc3",
  "preferGlobal": true,
  "main": "index.js",
  "license": "MIT",
  "author": "Runnerty Tech",
  "homepage": "http://www.runnerty.io/",
  "directories": {
    "bin": "./bin"
  },
  "bin": {
    "runnerty": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:runnerty/runnerty.git"
  },
  "bugs": {
    "url": "https://github.com/runnerty/runnerty/issues"
  },
  "scripts": {
    "test": "RUNNERTY_TEST=true jest --runInBand --detectOpenHandles --forceExit",
    "test-watch": "RUNNERTY_TEST=true jest --runInBand --detectOpenHandles --watch",
    "debug": "node --nolazy --inspect-brk index.js",
    "lint": "eslint '**/*.js' && prettier --check '**'"
  },
  "dependencies": {
    "@runnerty/interpreter-core": "~3.0.1",
    "@runnerty/trigger-server": "~3.0.5",
    "ajv": "~8.1.0",
    "axios": "~0.21.1",
    "better-ajv-errors": "~0.7.0",
    "body-parser": "~1.19.0",
    "boxen": "~5.0.1",
    "bytes": "~3.1.0",
    "chalk": "~4.1.0",
    "colors": "^1.4.0",
    "commander": "~7.2.0",
    "cors": "~2.8.5",
    "cron-parser": "^3.3.0",
    "express": "~4.17.1",
    "express-basic-auth": "~1.2.0",
    "express-jwt": "~6.0.0",
    "fs-extra": "~9.1.0",
    "glob-to-regexp": "~0.4.1",
    "helmet": "~4.4.1",
    "json-stringify-safe": "~5.0.1",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.21",
    "moment": "~2.29.1",
    "morgan": "~1.10.0",
    "ms": "~2.1.3",
    "node-ical": "~0.13.0",
    "npm-package-arg": "^8.1.2",
    "object-sizeof": "~1.6.1",
    "redis": "~3.1.1",
    "resolve": "^1.20.0",
    "semver": "~7.3.5",
    "socket.io-client": "~4.0.1",
    "update-notifier": "~5.1.0",
    "winston": "~3.3.3"
  },
  "keywords": [
    "runnerty",
    "scheduler",
    "task",
    "task manager",
    "automation",
    "job",
    "notifications",
    "executions",
    "sysadmin",
    "tools",
    "logs",
    "log",
    "reload",
    "programmatic",
    "production",
    "monitoring",
    "process manager",
    "profiling",
    "cron",
    "cronjob",
    "devops",
    "dev ops",
    "process orchestrator",
    "orchestrator",
    "orchestrating-processes",
    "workflow-automation"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.2.1",
    "runnerty-tests-modules": "file:__tests__/end2end"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "test_src"
    ],
    "collectCoverageFrom": [
      "lib/loadConfigSection.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
