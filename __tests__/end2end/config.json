{
  "$schema": "https://raw.githubusercontent.com/runnerty/schemas/master/schemas/3.0/config.json",
  "general": {
    "planFilePath": "plan.json",
    "runnerty.io": {
      "host": "https://ws-telemetry.runnerty.io/v3",
      "apikey": "YOUR_APIKEY_PROJECT",
      "healthChecker": true,
      "remoteControl": true,
      "debug": false,
      "disable": true
    },
    "queues": {
      "refreshInterval": 100
    }
  },
  "triggers": [
    {
      "id": "schedule_default",
      "type": "@runnerty-trigger-schedule"
    }
  ],
  "executors": [
    {
      "id": "iterable_default",
      "type": "@runnerty-executor-iterable"
    },
    {
      "id": "shell_default",
      "type": "@runnerty-executor-shell"
    },
    {
      "id": "wait_default",
      "type": "@runnerty-executor-wait",
      "time": "60s"
    }
  ],
  "notifiers": [
    {
      "id": "console_default",
      "type": "@runnerty-notifier-console"
    }
  ],
  "global_values": [
    {
      "myparams": {
        "sample": "sample_value"
      }
    }
  ]
}
