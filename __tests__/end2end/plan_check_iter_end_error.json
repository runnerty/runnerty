            "seconds": "2"
          },
          "notifications": {
            "on_start": [
              {
                "id": "console_default",
                "message": "-> -> [6-@GV(CMDPROC)-1] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) START"
              }
            ],
            "on_fail": [
              {
                "id": "console_default",
                "message": "ERR! [I:@GV(CMDPROC)] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) FAIL",
