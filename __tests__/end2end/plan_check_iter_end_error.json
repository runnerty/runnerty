{
  "chains": [
    {
      "id": "CHAIN-LAUNCHER",
      "name": "CHAIN-LAUNCHER",
      "notifications": {
        "on_start": [
          {
            "id": "console_default",
            "message": "1 CHAIN @GV(CHAIN_ID) START"
          }
        ],
        "on_end": [
          {
            "id": "console_default",
            "message": "10 CHAIN @GV(CHAIN_ID) END"
          }
        ],
        "on_fail": [
          {
            "id": "console_default",
            "message": "ERR! CHAIN @GV(CHAIN_ID) FAIL",
            "mode": "error"
          }
        ]
      },
      "processes": [
        {
          "id": "PROC-1",
          "name": "PROC-1",
          "exec": {
            "id": "shell_default",
            "command": "echo @GV(PROCESS_ID)"
          },
          "notifications": {
            "on_start": [
              {
                "id": "console_default",
                "message": "2 Inicio: @GV(PROCESS_ID)",
                "mode": "info"
              }
            ],
            "on_fail": [
              {
                "id": "console_default",
                "message": "Error: @GV(PROCESS_ID) @GV(PROCESS_EXEC_ERR_OUTPUT)",
                "mode": "error"
              }
            ],
            "on_end": [
              {
                "id": "console_default",
                "message": "3 Fin: @GV(PROCESS_ID)",
                "mode": "info"
              }
            ]
          }
        },
        {
          "id": "PROCESS-LAUNCHER-2",
          "name": "PROCESS-LAUNCHER-2",
          "depends_process": ["PROC-1"],
          "exec": {
            "id": "iterable_default",
            "objects": [
              {
                "CMDPROC": "echo @GV(PROCESS_ID)_1"
              },
              {
                "CMDPROC": "lol @GV(PROCESS_ID)_2"
              },
              {
                "CMDPROC": "echo @GV(PROCESS_ID)_3"
              }
            ]
          },
          "fail_on_child_fail": true,
          "notifications": {
            "on_start": [
              {
                "id": "console_default",
                "message": "4 Inicio: @GV(PROCESS_ID)",
                "mode": "info"
              }
            ],
            "on_fail": [
              {
                "id": "console_default",
                "message": "Error: @GV(PROCESS_ID) @GV(PROCESS_EXEC_ERR_OUTPUT)",
                "mode": "error"
              }
            ],
            "on_end": [
              {
                "id": "console_default",
                "message": "5 Fin: @GV(PROCESS_ID)",
                "mode": "info"
              }
            ]
          },
          "output_iterable": "PROCESS_EXEC_DATA_OUTPUT"
        },
        {
          "id": "PROCESS-LAUNCHER-3",
          "name": "PROCESS-LAUNCHER-3",
          "depends_process": ["PROCESS-LAUNCHER-2"],
          "exec": {
            "id": "iterable_default",
            "objects": [
              {
                "CMDPROC": "1"
              },
              {
                "CMDPROC": "2"
              },
              {
                "CMDPROC": "3"
              }
            ]
          },
          "notifications": {
            "on_start": [
              {
                "id": "console_default",
                "message": "6 Inicio: @GV(PROCESS_ID)",
                "mode": "info"
              }
            ],
            "on_fail": [
              {
                "id": "console_default",
                "message": "Error: @GV(PROCESS_ID) @GV(PROCESS_EXEC_ERR_OUTPUT)",
                "mode": "error"
              }
            ],
            "on_end": [
              {
                "id": "console_default",
                "message": "7 Fin: @GV(PROCESS_ID) @GV(PROCESS_EXEC_MSG_OUTPUT)",
                "mode": "info"
              }
            ]
          },
          "output_iterable": "PROCESS_EXEC_DATA_OUTPUT"
        },
        {
          "id": "PROC-FIN",
          "name": "PROC-FIN",
          "depends_process": ["PROCESS-LAUNCHER-2"],
          "exec": {
            "id": "shell_default",
            "command": "echo @GV(PROCESS_ID)"
          },
          "notifications": {
            "on_start": [
              {
                "id": "console_default",
                "message": "8 Inicio: @GV(PROCESS_ID)",
                "mode": "info"
              }
            ],
            "on_fail": [
              {
                "id": "console_default",
                "message": "Error: @GV(PROCESS_ID) @GV(PROCESS_EXEC_ERR_OUTPUT)",
                "mode": "error"
              }
            ],
            "on_end": [
              {
                "id": "console_default",
                "message": "9 Fin: @GV(PROCESS_ID)",
                "mode": "info"
              }
            ]
          }
        }
      ]
    },
    {
      "id": "CHAIN-ITERABLE-2",
      "name": "CHAIN-ITERABLE-2",
      "iterable": "serie",
      "abort_iteration_serie_on_error": true,
      "depends_chains": {
        "chain_id": "CHAIN-LAUNCHER",
        "process_id": "PROCESS-LAUNCHER-2"
      },
      "input": [
        {
          "CMDPROC": "CMDPROC"
        }
      ],
      "notifications": {
        "on_start": [
          {
            "id": "console_default",
            "message": "-> [5-@GV(CMDPROC)] CHAIN @GV(CHAIN_ID) START"
          }
        ],
        "on_end": [
          {
            "id": "console_default",
            "message": "-> [5-@GV(CMDPROC)] CHAIN @GV(CHAIN_ID) END"
          }
        ],
        "on_fail": [
          {
            "id": "console_default",
            "message": "ERR CHAIN! [I:@GV(CMDPROC)] CHAIN @GV(CHAIN_ID) FAIL",
            "mode": "error"
          }
        ]
      },
      "processes": [
        {
          "id": "PROCESS-ITER-2-2",
          "name": "PROCESS-ITER-2-2",
          "exec": {
            "id": "shell_default",
            "command": "@GV(CMDPROC)"
          },
          "notifications": {
            "on_start": [
              {
                "id": "console_default",
                "message": "-> -> [5-@GV(CMDPROC)-1] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) START"
              }
            ],
            "on_fail": [
              {
                "id": "console_default",
                "message": "ERR! [I:@GV(CMDPROC)] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) FAIL",
                "mode": "error"
              }
            ],
            "on_end": [
              {
                "id": "console_default",
                "message": "-> -> [5-@GV(CMDPROC)-2] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) END"
              }
            ]
          }
        },
        {
          "id": "PROCESS-ITER-2-3",
          "name": "PROCESS-ITER-2-3",
          "exec": {
            "id": "shell_default",
            "command": "echo @GV(PROCESS_ID)"
          },
          "depends_process": ["PROCESS-ITER-2-2"],
          "notifications": {
            "on_start": [
              {
                "id": "console_default",
                "message": "-> -> [5-@GV(CMDPROC)-3] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) START"
              }
            ],
            "on_fail": [
              {
                "id": "console_default",
                "message": "ERR! [I:@GV(CMDPROC)] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) FAIL",
                "mode": "error"
              }
            ],
            "on_end": [
              {
                "id": "console_default",
                "message": "-> -> [5-@GV(CMDPROC)-4] PROCESS @GV(PROCESS_ID) OF CHAIN @GV(CHAIN_ID) END"
              }
            ]
          }
        }
      ]
    },
    {
      "id": "CHAIN-ITERABLE-3",
      "name": "CHAIN-ITERABLE-3",
      "iterable": "serie",
      "depends_chains": {
        "chain_id": "CHAIN-LAUNCHER",
        "process_id": "PROCESS-LAUNCHER-3"
      },
      "input": [
        {
          "CMDPROC": "CMDPROC"
        }
      ],
      "notifications": {
        "on_start": [
          {
            "id": "console_default",
            "message": "-> [6-@GV(CMDPROC)] CHAIN @GV(CHAIN_ID) START"
          }
        ],
        "on_end": [
          {
            "id": "console_default",
            "message": "-> [6-@GV(CMDPROC)] CHAIN @GV(CHAIN_ID) END"
          }
