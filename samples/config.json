{
  "general": {
    "binBackup": "/tmp/bin.json",
    "planFilePath": "/etc/runnerty/plan.json",
    "refreshIntervalBinBackup": 2000,
    "api": {
      "port": 3456,
      "users": [
        {
          "user": "coderty",
          "password": "password_runnerty"
        },
        {
          "user": "usr_test",
          "password": "pass_test"
        }
      ],
      "secret": "RUNNERTY_BY_CODERTY",
      "limite_req": "20mb",
      "chainsFieldsResponse": [
        "id",
        "name",
        "status",
        "started_at",
        "ended_at",
        "duration_humanized",
        "custom_values",
        "processes.id",
        "processes.name",
        "processes.status",
        "processes.retries",
        "processes.started_at",
        "processes.ended_at",
        "processes.duration_humanized"
      ],
      "processFieldsResponse": ["id", "name", "status", "retries", "started_at", "ended_at", "duration_humanized"]
    },
    "calendarsPath": "/calendars_dir/",
    "servers": [
      {
        "id": "srvId",
        "port": 8080,
        "endpoint": "/test_one"
      },
      {
        "id": "srvIdTwo",
        "port": 8181,
        "endpoint": "/test_two"
      }
    ],
    "runnerty.io": {
      "apikey": "",
      "disable": true,
      "healthChecker": true
    },
    "queue_notifications": {
      "queue": "redis",
      "host": "127.0.0.1",
      "port": "32768",
      "password": "redis_password",
      "options": {}
    }
  },
  "triggers": [
    {
      "id": "schedule_default",
      "type": "@runnerty-trigger-schedule"
    },
    {
      "id": "filewatcher_default",
      "type": "@runnerty-trigger-file-watcher"
    }
  ],
  "executors": [
    {
      "id": "scp_default",
      "type": "@runnerty-executor-scp",
      "identityFile": "/.ssh/my.pem"
    },
    {
      "id": "s3_default",
      "type": "@runnerty-executor-s3",
      "apiVersion": "2006-03-01",
      "accessKeyId": "ABC123",
      "secretAccessKey": "ABC123",
      "bucket": "my.s3bucket.com",
      "method": "upload",
      "region": "eu-west-1"
    },
    {
      "id": "mail_default",
      "type": "@runnerty-executor-mail",
      "disable": false,
      "from": "Runnerty <my@mail.com>",
      "transport": "smtps://my%40mail.com:mypass@smtp.mail.com/?pool=true",
      "templateDir": "/etc/runnerty/templates",
      "template": "alerts",
      "to": ["to@mail.com"],
      "ejsRender": true
    },
    {
      "id": "iconv_default",
      "type": "@runnerty-executor-iconv"
    },
    {
      "id": "shell_default",
      "type": "@runnerty-executor-shell"
    },
    {
      "id": "wait_default",
      "type": "@runnerty-executor-wait",
      "seconds": "60"
    },
    {
      "id": "mysql_default",
      "type": "@runnerty-executor-mysql",
      "user": "mysql_user",
      "password": "mysql_password",
      "_crypted_password": "mysql_crypted_password",
      "database": "MYDB",
      "host": "mysqlhost.com",
      "port": "3306"
    },
    {
      "id": "postgres_default",
      "type": "@runnerty-executor-postgres",
      "user": "postgres_user",
      "password": "postgres_password",
      "database": "MYDB",
      "host": "postgreshost.com",
      "port": "5439"
    },
    {
      "id": "redis_default",
      "type": "@runnerty-executor-redis",
      "password": "redis_password",
      "host": "redishost.com",
      "port": "6379",
      "options": {}
    }
  ],
  "notifiers": [
    {
      "id": "slack_default",
      "type": "@runnerty-notifier-slack",
      "webhookurl": "https://hooks.slack.com/services/ABC123/ABC123/ABC123",
      "bot_name": "Runnerty-Sentinel",
      "channel": "my_runnerty_channel",
      "maxConcurrents": 1,
      "minInterval": 0
    },
    {
      "id": "mail_default",
      "type": "@runnerty-notifier-mail",
      "disable": false,
      "from": "Runnerty <my@mail.com>",
      "transport": "smtps://my%40mail.com:mypass@smtp.mail.com/?pool=true",
      "templateDir": "/etc/runnerty/templates",
      "template": "alerts",
      "to": ["to@mail.com"],
      "ejsRender": true
    },
    {
      "id": "telegram_default",
      "type": "@runnerty-notifier-telegram",
      "token": "ABC123",
      "chat_id": "ABC123"
    }
  ],
  "global_values": [
    {
      "myparams": {
        "sample": "coderty.com"
      }
    },
    {
      "myothersparams": {
        "other_sample": "runnerty"
      }
    },
    {
      "MY_LISTS": {
        "LIST_ZERO": {
          "value": ["@GETDATE(YYYY)", "-CA-", "@GETDATE(MM)", "-TO-", "@GETDATE(DD)"],
          "format": "text"
        },
        "LIST_ONE": {
          "value": ["VAL1", "VAL2@GETDATE(MM)", "VAL3"],
          "format": "text",
          "delimiter": ",",
          "quotechar": "\""
        },
        "LIST_TWO": { "value": ["VAL1", "VAL2", "VAL3"], "format": "json" },
        "LIST_THREE": {
          "value": [
            { "KEY1": "VAL1", "KEY2": "VAL2" },
            { "KEY1": "VAL1", "KEY2": "VAL2" }
          ],
          "format": "json"
        }
      }
    }
  ]
}
