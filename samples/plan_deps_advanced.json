{
  "chains": [
    {
      "id": "CHAIN_ONE",
      "name": "Chain one sample",
      "triggers": [
        {
          "id": "schedule_default",
          "start_date": "2017-09-05T00:00:00.00Z",
          "end_date": "2099-09-05T00:00:00.00Z",
          "schedule_interval": "*/1 * * * *"
        },
        {
          "id": "filewatcher_default",
          "file_name": "/etc/runnerty/myfile.txt",
          "condition": "add"
        }
      ],
      "depends_chains": [],
      "notifications": {
        "on_start": [
          {
            "id": "mail_default",
            "subject": "RUNNERTY SAMPLE",
            "message": "CHAIN :CHAIN_NAME START"
          }
        ],
        "on_end": [
          {
            "id": "mail_default",
            "subject": "RUNNERTY SAMPLE",
            "message": "CHAIN :CHAIN_NAME END"
          }
        ]
      },
      "processes": [
        {
          "id": "PROCESS_ONE",
          "name": "Process One",
          "depends_process": {},
          "exec": {
            "id": "shell_default",
            "command": "echo hello word from :PROCESS_ID!"
          },
          "retries": 0,
          "retry_delay": 0,
          "chain_action_on_fail": "abort",
          "output": [
            {
              "file_name": "/etc/runnerty/test.log",
              "write": ["> EXEC :PROCESS_ID - @GETDATE('DD-MM-YY HH:mm:ss') RETURN: @GV(PROCESS_EXEC_MSG_OUTPUT)"],
              "concat": true,
              "maxsize": "10mb"
            }
          ],
          "notifications": {
            "on_start": [
              {
                "id": "mail_default",
                "subject": "RUNNERTY SAMPLE",
                "message": "PROCESS :PROCESS_NAME START"
              }
            ],
            "on_fail": [],
            "on_retry": [],
            "on_end": [
              {
                "id": "mail_default",
                "subject": "RUNNERTY SAMPLE",
                "message": "PROCESS :PROCESS_NAME END. RETURN: @GV(PROCESS_EXEC_MSG_OUTPUT)"
              }
            ]
          }
        },
        {
          "id": "PROCESS_TWO",
          "name": "Process Two",
          "depends_process": { "$and": [{ "$end": "PROCESS_ONE" }, { "ABC": { "$eq": "ABC" } }] },
          "exec": {
            "id": "shell_default",
            "command": "echo hello word from @GV(PROCESS_ID)!"
          },
          "retries": 0,
          "retry_delay": 0,
          "chain_action_on_fail": "abort",
          "output": [
            {
              "file_name": "/etc/runnerty/test.log",
              "write": ["> EXEC :PROCESS_ID - @GETDATE('DD-MM-YY HH:mm:ss') RETURN: @GV(PROCESS_EXEC_MSG_OUTPUT)"],
              "concat": true,
              "maxsize": "10mb"
            }
          ],
          "notifications": {
            "on_start": [
              {
                "id": "mail_default",
                "subject": "RUNNERTY SAMPLE",
                "message": "PROCESS @GV(PROCESS_NAME) START"
              }
            ],
            "on_fail": [],
            "on_retry": [],
            "on_end": [
              {
                "id": "mail_default",
                "subject": "RUNNERTY SAMPLE",
                "message": "PROCESS @GV(PROCESS_NAME) END. RETURN: @GV(PROCESS_EXEC_MSG_OUTPUT)"
              }
            ],
            "on_ignore": [
              {
                "id": "telegram_default",
                "message": ">  > PROCESS IGNORED *@GV(PROCESS_NAME)* "
              }
            ]
          }
        }
      ]
    }
  ]
}
