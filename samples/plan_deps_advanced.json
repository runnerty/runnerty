{
  "chains":[
    {
      "id":"CHAIN_ONE",
      "name":"Chain one sample",
      "start_date":"2017-04-01T00:00:00.00Z",
      "end_date":"2099-11-01T00:00:00.00Z",
      "schedule_interval":"*/1 * * * *",
      "depends_chains":[],
      "events":{
        "on_start":{
          "notifications":[
            {
              "id":"mail_default",
              "subject":"RUNNERTY SAMPLE",
              "message":"CHAIN :CHAIN_NAME START"
            }
          ]
        },
        "on_end":{
          "notifications":[
            {
              "id":"mail_default",
              "subject":"RUNNERTY SAMPLE",
              "message":"CHAIN :CHAIN_NAME END"
            }
          ]
        }
      },
      "processes":[
        {
          "id":"PROCESS_ONE",
          "name":"Proccess One",
          "depends_process":[],
          "exec":{
            "id":"shell_default",
            "command":"echo hello word from :PROCESS_ID!"
          },
          "retries":0,
          "retry_delay":0,
          "end_chain_on_fail":true,
          "output":[
            {
              "file_name":"/etc/runnerty/test.log",
              "write":[
                "> EXEC :PROCESS_ID - :DD-:MM-:YY :HH::mm::ss: RETURN: :PROCESS_EXEC_RETURN"
              ],
              "concat":true,
              "maxsize":"10mb"
            }
          ],
          "events":{
            "on_start":{
              "notifications":[
                {
                  "id":"mail_default",
                  "subject":"RUNNERTY SAMPLE",
                  "message":"PROCESS :PROCESS_NAME START"
                }
              ]
            },
            "on_fail":{},
            "on_retry":{},
            "on_end":{
              "notifications":[
                {
                  "id":"mail_default",
                  "subject":"RUNNERTY SAMPLE",
                  "message":"PROCESS :PROCESS_NAME END. RETURN: :PROCESS_EXEC_RETURN"
                }
              ]
            }
          }
        },
        {
          "id":"PROCESS_TWO",
          "name":"Proccess Two",
          "depends_process":[{"id":"PROCESS_ONE","evaluate": [{ "oper_left": "1", "condition": "==", "oper_right": "1" }]}],
          "depends_alt":[],
          "depends_conditions":[{ "oper_left": "1", "condition": "==", "oper_right": "1" }],
          "exec":{
            "id":"shell_default",
            "command":"echo hello word from :PROCESS_ID!"
          },
          "retries":0,
          "retry_delay":0,
          "end_chain_on_fail":true,
          "output":[
            {
              "file_name":"/etc/runnerty/test.log",
              "write":[
                "> EXEC :PROCESS_ID - :DD-:MM-:YY :HH::mm::ss: RETURN: :PROCESS_EXEC_RETURN"
              ],
              "concat":true,
              "maxsize":"10mb"
            }
          ],
          "events":{
            "on_start":{
              "notifications":[
                {
                  "id":"mail_default",
                  "subject":"RUNNERTY SAMPLE",
                  "message":"PROCESS :PROCESS_NAME START"
                }
              ]
            },
            "on_fail":{},
            "on_retry":{},
            "on_end":{
              "notifications":[
                {
                  "id":"mail_default",
                  "subject":"RUNNERTY SAMPLE",
                  "message":"PROCESS :PROCESS_NAME END. RETURN: :PROCESS_EXEC_RETURN"
                }
              ]
            },
            "on_ignore":{
              "notifications":[
                {
                  "id":"telegram_default",
                  "message":">  > IGNORADO EL PROCESO *:PROCESS_NAME* "
                }
              ]
            }
          }
        }
      ]
    }
  ]
}