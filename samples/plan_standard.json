{
  "chains": [
    {
      "id": "CHAIN_ONE",
      "name": "Chain one sample",
      "depends_chains": [],
      "triggers": [
        {
          "id": "schedule_default",
          "start_date": "2017-09-05T00:00:00.00Z",
          "end_date": "2099-09-05T00:00:00.00Z",
          "schedule_interval": "*/1 * * * *"
        }
      ],
      "defaults_processes": {
        "notifications": {
          "on_start": [
            {
              "id": "mail_default",
              "subject": "RUNNERTY SAMPLE",
              "message": "PROCESS @GV(PROCESS_NAME) START"
            }
          ],
          "on_fail": [],
          "on_retry": [],
          "on_end": [
            {
              "id": "mail_default",
              "subject": "RUNNERTY SAMPLE",
              "message": "PROCESS @GV(PROCESS_NAME) END. RETURN: @GV(PROCESS_EXEC_MSG_OUTPUT)"
            }
          ]
        }
      },
      "notifications": {
        "on_start": [
          {
            "id": "mail_default",
            "subject": "RUNNERTY SAMPLE",
            "message": "CHAIN @GV(CHAIN_NAME) START"
          }
        ],
        "on_end": [
          {
            "id": "mail_default",
            "subject": "RUNNERTY SAMPLE",
            "message": "CHAIN @GV(CHAIN_NAME) END"
          }
        ]
      },
      "processes": [
        {
          "id": "PROCESS_ONE",
          "name": "Process One",
          "depends_process": {},
          "exec": {
            "id": "shell_default",
            "command": "echo hello word from @GV(PROCESS_ID)!"
          },
          "retries": 0,
          "retry_delay": 0,
          "chain_action_on_fail": "abort",
          "output": [
            {
              "file_name": "/etc/runnerty/test.log",
              "write": ["> EXEC :PROCESS_ID - @GETDATE('DD-MM-YY HH:mm:ss') RETURN: @GV(PROCESS_EXEC_MSG_OUTPUT)"],
              "concat": true,
              "maxsize": "10mb"
            }
          ]
        },
        {
          "id": "PROCESS_TWO",
          "name": "Process Two",
          "depends_process": "PROCESS_ONE",
          "exec": {
            "id": "shell_default",
            "command": "echo hello word from @GV(PROCESS_ID)!"
          },
          "retries": 0,
          "retry_delay": 0,
          "chain_action_on_fail": "abort",
          "output": [
            {
              "file_name": "/etc/runnerty/test.log",
              "write": ["> EXEC :PROCESS_ID - @GETDATE('DD-MM-YY HH:mm:ss') RETURN: @GV(PROCESS_EXEC_MSG_OUTPUT)"],
              "concat": true,
              "maxsize": "10mb"
            }
          ]
        }
      ]
    }
  ]
}
