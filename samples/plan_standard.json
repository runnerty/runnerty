{
  "chains":[
    {
      "id":"CHAIN_ONE",
      "name":"Chain one sample",
      "start_date":"2017-04-01T00:00:00.00Z",
      "end_date":"2099-11-01T00:00:00.00Z",
      "schedule_interval":"*/1 * * * *",
      "depends_chains":[],
      "notifications":{
        "on_start":[
          {
            "id":"mail_default",
            "subject":"RUNNERTY SAMPLE",
            "message":"CHAIN :CHAIN_NAME START"
          }
        ],
        "on_end":[
          {
            "id":"mail_default",
            "subject":"RUNNERTY SAMPLE",
            "message":"CHAIN :CHAIN_NAME END"
          }
        ]
      },
      "processes":[
        {
          "id":"PROCESS_ONE",
          "name":"Process One",
          "depends_process":[],
          "exec":{
            "id":"shell_default",
            "command":"echo hello word from :PROCESS_ID!"
          },
          "retries":0,
          "retry_delay":0,
          "end_chain_on_fail":true,
          "output":[
            {
              "file_name":"/etc/runnerty/test.log",
              "write":[
                "> EXEC :PROCESS_ID - :DD-:MM-:YY :HH::mm::ss: RETURN: :PROCESS_EXEC_MSG_OUTPUT"
              ],
              "concat":true,
              "maxsize":"10mb"
            }
          ],
          "notifications":{
            "on_start":[
              {
                "id":"mail_default",
                "subject":"RUNNERTY SAMPLE",
                "message":"PROCESS :PROCESS_NAME START"
              }
            ],
            "on_fail":[],
            "on_retry":[],
            "on_end":[
              {
                "id":"mail_default",
                "subject":"RUNNERTY SAMPLE",
                "message":"PROCESS :PROCESS_NAME END. RETURN: :PROCESS_EXEC_MSG_OUTPUT"
              }
            ]
          }
        },
        {
          "id":"PROCESS_TWO",
          "name":"Process Two",
          "depends_process":["PROCESS_ONE"],
          "exec":{
            "id":"shell_default",
            "command":"echo hello word from :PROCESS_ID!"
          },
          "retries":0,
          "retry_delay":0,
          "end_chain_on_fail":true,
          "output":[
            {
              "file_name":"/etc/runnerty/test.log",
              "write":[
                "> EXEC :PROCESS_ID - :DD-:MM-:YY :HH::mm::ss: RETURN: :PROCESS_EXEC_MSG_OUTPUT"
              ],
              "concat":true,
              "maxsize":"10mb"
            }
          ],
          "notifications":{
            "on_start":[
              {
                "id":"mail_default",
                "subject":"RUNNERTY SAMPLE",
                "message":"PROCESS :PROCESS_NAME START"
              }
            ],
            "on_fail":[],
            "on_retry":[],
            "on_end":[
              {
                "id":"mail_default",
                "subject":"RUNNERTY SAMPLE",
                "message":"PROCESS :PROCESS_NAME END. RETURN: :PROCESS_EXEC_MSG_OUTPUT"
              }
            ]
          }
        }
      ]
    }
  ]
}