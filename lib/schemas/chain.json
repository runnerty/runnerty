{
  "type": "object",
  "required": ["id","name","processes"],
  "properties": {
    "id": {"type": "string"},
    "name": {"type": "string"},
    "triggers": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "calendars": {
            "type": "object",
            "properties": {
              "enable":{"type": "string"},
              "disable":{"type": "string"}
            }
          }
        }
      }
    },
    "depends_chains": {"type": ["array","object"]},
    "notifications": {
      "type": "object",
