          }
        },
        "on_waiting_dependencies": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["id"],
            "properties": {
              "id": {"type": "string"}
            }
          }
        }
      }
    },
    "processes": {"$ref": "processSchema"}
